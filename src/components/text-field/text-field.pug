mixin text-field({ type = 'text', name, placeholder, typeOfField })
    case typeOfField
        when 'masked'
            input.text-field.js-text-field_masked(type=type, name=name, placeholder='ДД.ММ.ГГГГ')
    case typeOfField
        when 'normal'
            input.text-field(type=type, name=name, placeholder=placeholder)
