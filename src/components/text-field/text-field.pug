mixin text-field({ type = 'text', name, placeholder, typeOfField })
    case typeOfField
        when 'masked'
            input(type=type, name=name, placeholder='ДД.ММ.ГГГГ', class= `text-field js-text-field_masked`)
    case typeOfField
        when 'normal'
            input(type=type, name=name, placeholder=placeholder, class= 'text-field')
