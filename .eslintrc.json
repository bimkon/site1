{
    "env": {
        "browser": true,
        "es2021": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
       "indent": ["error", 4],
        "no-param-reassign": "off",
        "no-var": "off",
        "no-redeclare": "off",
        "no-plusplus": "off",
        "import/extensions": "off"
    }
}
